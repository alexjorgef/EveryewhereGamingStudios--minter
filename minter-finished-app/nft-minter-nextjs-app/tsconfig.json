{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "sourceMap": true,
    "outDir": "dist",
    "noImplicitAny": true
  },
  "include": [
    "artifacts/**/*",
    "artifacts/**/*.json",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "contract.ts",
    "hardhat.config.ts"
  ],
  "exclude": ["node_modules"]
}
